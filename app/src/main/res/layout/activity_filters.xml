                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageSearch"
                    android:layout_height="@dimen/_44pxh"
                    android:layout_width="@dimen/_44pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_search_red_700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_search_red_700"/>
                <View
                    android:background="@drawable/rectangle_bg_gray_100_radius_18"
                    android:id="@+id/viewEllipseTwo"
                    android:layout_height="@dimen/_36pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_4pxv" android:layout_width="@dimen/_36pxh"/>
                <View
