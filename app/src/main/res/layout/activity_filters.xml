                            android:id="@+id/txtHeadline"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_135pxh"
                            android:layout_weight="0.86"
                            android:layout_width="0dp"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{filtersVM.filtersModel.txtHeadline}"
                            style="@style/txtMetropolissemibold18" tools:text="@string/lbl_filters"/>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtGroup112"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_14pxv"
                android:singleLine="true"
                android:text="@{filtersVM.filtersModel.txtGroup112}"
                style="@style/txtSolid_1" tools:text="@string/lbl_price_range"/>
            <LinearLayout android:id="@+id/linearRowprice"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_26pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPrice"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.45"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{filtersVM.filtersModel.txtPrice}"
                    style="@style/txtMetropolismedium14" tools:text="@string/lbl_78"/>
                <TextView android:ellipsize="end"
