                <EditText
                    android:background="@drawable/rectangle_bg_white_a700_border_gray_500_radius_8"
                    android:ellipsize="end"
                    android:hint="@string/lbl_women"
                    android:id="@+id/etTagInactiveOrThree"
                    android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_22pxh"
                    android:layout_weight="0.33"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_24pxh"
                    android:paddingStart="@dimen/_23pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@={filtersVM.filtersModel.etTagInactiveOrThreeValue}"
                    android:textColorHint="@color/gray_900"
                    style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
                <EditText
