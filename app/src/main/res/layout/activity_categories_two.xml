            <TextView android:ellipsize="end"
                android:id="@+id/txtCardigansSwe"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_40pxh"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{categoriesTwoVM.categoriesTwoModel.txtCardigansSwe}"
                style="@style/txtMetropolisregular16" tools:text="@string/msg_cardigans_swe"/>
            <View android:background="@color/gray_500_71"
                android:id="@+id/lineLineTwo"
                android:layout_gravity="start"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_375pxh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtKnitwear"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_40pxh"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{categoriesTwoVM.categoriesTwoModel.txtKnitwear}"
                style="@style/txtMetropolisregular16" tools:text="@string/lbl_knitwear"/>
            <View android:background="@color/gray_500_71"
                android:id="@+id/lineLineThree"
                android:layout_gravity="start"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_375pxh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtBlazers"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_40pxh"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{categoriesTwoVM.categoriesTwoModel.txtBlazers}"
                style="@style/txtMetropolisregular16" tools:text="@string/lbl_blazers"/>
            <View android:background="@color/gray_500_71"
                android:id="@+id/lineLineFour"
                android:layout_gravity="start"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_375pxh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtOuterwear"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_40pxh"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{categoriesTwoVM.categoriesTwoModel.txtOuterwear}"
                style="@style/txtMetropolisregular16" tools:text="@string/lbl_outerwear"/>
            <View android:background="@color/gray_500_71"
                android:id="@+id/lineLineFive"
                android:layout_gravity="start"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_18pxv" android:layout_width="@dimen/_375pxh"/>
            <TextView android:ellipsize="end" android:id="@+id/txtPants"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_40pxh"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{categoriesTwoVM.categoriesTwoModel.txtPants}"
                style="@style/txtMetropolisregular16" tools:text="@string/lbl_pants"/>
            <View android:background="@color/gray_500_71"
                android:id="@+id/lineLineSix"
                android:layout_gravity="start"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_375pxh"/>
            <TextView android:ellipsize="end" android:id="@+id/txtJeans"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_40pxh"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{categoriesTwoVM.categoriesTwoModel.txtJeans}"
                style="@style/txtMetropolisregular16" tools:text="@string/lbl_jeans"/>
            <View android:background="@color/gray_500_71"
                android:id="@+id/lineLineSeven"
                android:layout_gravity="start"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_375pxh"/>
            <TextView android:ellipsize="end"
