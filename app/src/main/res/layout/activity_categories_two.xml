                android:layout_gravity="start"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_375pxh"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSkirts"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_40pxh"
                android:layout_marginStart="@dimen/_40pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{categoriesTwoVM.categoriesTwoModel.txtSkirts}"
                style="@style/txtMetropolisregular16" tools:text="@string/lbl_skirts"/>
