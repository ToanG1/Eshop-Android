<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="catalogOneRowModel" type="com.nguyenvansapplication.app.modules.catalogone.data.model.CatalogOneRowModel"/>
    </data>
    <FrameLayout android:id="@+id/frameProductcardLi"
        android:layout_height="wrap_content" android:layout_width="match_parent">
        <LinearLayout android:id="@+id/linearRowitem"
            android:layout_gravity="top|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadius">
            <FrameLayout android:id="@+id/framePhoto"
                android:layout_height="@dimen/_104pxv" android:layout_width="@dimen/_104pxh">
                <ImageView android:id="@+id/imageImage"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_104pxh"
                    android:layout_width="@dimen/_104pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_8pxh}"
                    imageRes="@{@drawable/img_image_104x104}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image_104x104"/>
                <FrameLayout android:id="@+id/frameStack"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_104pxv" android:layout_width="@dimen/_104pxh">
                    <ImageView android:id="@+id/imageImageOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_104pxh"
                        android:layout_width="@dimen/_104pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_8pxh}"
                        imageRes="@{@drawable/img_image_6}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image_6"/>
                    <ImageView android:id="@+id/imageImageTwo"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_104pxh"
                        android:layout_width="@dimen/_104pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_8pxh}"
                        imageRes="@{@drawable/img_image_7}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image_7"/>
                </FrameLayout>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnitem"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_137pxh"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_weight="0.46" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtItem"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{catalogOneRowModel.txtItem}"
                    style="@style/txtMetropolissemibold16" tools:text="@string/lbl_pullover"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBrand"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{catalogOneRowModel.txtBrand}"
                    style="@style/txtMetropolisregular11_1" tools:text="@string/lbl_mango"/>
                <LinearLayout android:id="@+id/linearRating"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageStar"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star_orange_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star_orange_300"/>
                    <ImageView android:id="@+id/imageStarOne"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star_orange_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star_orange_300"/>
                    <ImageView android:id="@+id/imageStarTwo"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star_orange_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star_orange_300"/>
                    <ImageView android:id="@+id/imageStarThree"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star_orange_300"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star_orange_300"/>
                    <ImageView android:id="@+id/imageStarFour"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtNumber"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_weight="0.17"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{catalogOneRowModel.txtNumber}"
                        style="@style/txtMetropolisregular10" tools:text="@string/lbl_3"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPrice"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{catalogOneRowModel.txtPrice}"
                    style="@style/txtMetropolismedium14" tools:text="@string/lbl_51"/>
            </LinearLayout>
        </LinearLayout>
        <ImageButton
            android:background="@drawable/rectangle_bg_white_a700_radius_18"
            android:id="@+id/btnClock"
            android:layout_gravity="bottom|end"
            android:layout_height="@dimen/_36pxh"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="@dimen/_36pxh"
            android:paddingHorizontal="@dimen/_11pxh"
            android:paddingVertical="@dimen/_11pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_clock" tools:ignore="ContentDescription"/>
    </FrameLayout>
</layout>
