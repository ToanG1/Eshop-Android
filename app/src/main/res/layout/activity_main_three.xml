<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="mainThreeVM" type="com.nguyenvansapplication.app.modules.mainthree.data.viewmodel.MainThreeVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnhome"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumn"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_50">
            <FrameLayout android:id="@+id/frameMain"
                android:layout_height="@dimen/_322pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageImageFour"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_322pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_image4_322x375}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image4_322x375"/>
                <LinearLayout
                    android:id="@+id/linearColumnnewcollection"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyle_1">
                    <TextView android:ellipsize="end"
                        android:gravity="end"
                        android:id="@+id/txtNewcollection"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_29pxv"
                        android:layout_marginEnd="@dimen/_18pxh"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_258pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{mainThreeVM.mainThreeModel.txtNewcollection}"
                        style="@style/txtMetropolisbold34_1" tools:text="@string/lbl_new_collection"/>
                </LinearLayout>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearRowtitle"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumntitleOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.5"
                    android:layout_width="0dp" android:orientation="vertical">
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumntitle"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylewhite_A700">
                        <TextView android:gravity="start"
                            android:id="@+id/txtTitle"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="@dimen/_58pxv"
                            android:layout_marginEnd="@dimen/_15pxh"
                            android:layout_marginStart="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_55pxv"
                            android:layout_width="match_parent"
                            android:text="@{mainThreeVM.mainThreeModel.txtTitle}"
                            style="@style/txtMetropolisbold34_2" tools:text="@string/lbl_summer_sale"/>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameStacktitleOne"
                        android:layout_height="@dimen/_187pxv" android:layout_width="@dimen/_187pxh">
                        <ImageView android:id="@+id/imageImageTwo"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_187pxh"
                            android:layout_width="@dimen/_187pxh"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_image2_187x187}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_image2_187x187"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTitleOne"
                            android:layout_gravity="bottom|start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_38pxv"
                            android:layout_marginEnd="@dimen/_13pxh"
                            android:layout_marginStart="@dimen/_13pxh"
                            android:layout_marginTop="@dimen/_38pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{mainThreeVM.mainThreeModel.txtTitleOne}"
                            style="@style/txtMetropolisbold34_1" tools:text="@string/lbl_black"/>
                    </FrameLayout>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameImage"
                    android:layout_height="@dimen/_374pxv" android:layout_width="@dimen/_188pxh">
                    <ImageView android:id="@+id/imageImageTwoOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_374pxh"
                        android:layout_width="@dimen/_188pxh"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_image2_374x188}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image2_374x188"/>
                    <LinearLayout android:id="@+id/linearColumntitleTwo"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_72pxv"
                        android:layout_marginTop="@dimen/_72pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStyle_2">
                        <TextView android:gravity="end"
                            android:id="@+id/txtTitleTwo"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="@dimen/_79pxv"
                            android:layout_marginEnd="@dimen/_18pxh"
                            android:layout_marginStart="@dimen/_18pxh"
                            android:layout_marginTop="@dimen/_52pxv"
                            android:layout_width="match_parent"
                            android:text="@{mainThreeVM.mainThreeModel.txtTitleTwo}"
                            style="@style/txtMetropolisbold34_1" tools:text="@string/lbl_men_s_hoodies"/>
                    </LinearLayout>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylewhite_9">
            <LinearLayout android:id="@+id/linearTabs"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_33pxv"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearTab1Main"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageHome"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home_active"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home_active"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtLabel"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{mainThreeVM.mainThreeModel.txtLabel}"
                        style="@style/txtMetropolissemibold10" tools:text="@string/lbl_home"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearTab2Shop"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageCart"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_cart"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_cart"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtLabelOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{mainThreeVM.mainThreeModel.txtLabelOne}"
                        style="@style/txtMetropolisregular10" tools:text="@string/lbl_shop"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearTab3Bag"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageSearch"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_23pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search_gray_500"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_search_gray_500"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtLabelTwo"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{mainThreeVM.mainThreeModel.txtLabelTwo}"
                        style="@style/txtMetropolisregular10" tools:text="@string/lbl_bag"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearTab4Favorit"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageFavorite"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_26pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_favorite"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_favorite"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtLabelThree"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{mainThreeVM.mainThreeModel.txtLabelThree}"
                        style="@style/txtMetropolisregular10" tools:text="@string/lbl_favorites"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearTab5MyProf"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="1" android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:id="@+id/imageUser"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtLabelFour"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{mainThreeVM.mainThreeModel.txtLabelFour}"
                        style="@style/txtMetropolisregular10" tools:text="@string/lbl_profile"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
