<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="mainPageRowModel" type="com.nguyenvansapplication.app.modules.mainpage.data.model.MainPageRowModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearProductcardne"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" android:orientation="vertical">
        <FrameLayout android:id="@+id/frameStacknew1"
            android:layout_height="@dimen/_201pxv" android:layout_width="@dimen/_150pxh">
            <FrameLayout android:id="@+id/framePhoto"
                android:layout_gravity="top|center"
                android:layout_height="@dimen/_184pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginStart="@dimen/_2pxh" android:layout_width="@dimen/_148pxh">
                <ImageView android:id="@+id/imageImageTwo"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_184pxh"
                    android:layout_width="@dimen/_148pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_4pxh}"
                    tools:ignore="ContentDescription" />
                <FrameLayout android:id="@+id/frameStacknew2"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_184pxv" android:layout_width="@dimen/_148pxh">
                    <ImageView android:id="@+id/imageImageOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_184pxh"
                        android:layout_width="@dimen/_148pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_4pxh}"
                        tools:ignore="ContentDescription"
                        />
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnNew"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_24pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="@dimen/_40pxh"
                        android:paddingBottom="@dimen/_6pxv"
                        android:paddingEnd="@dimen/_6pxh"
                        android:paddingStart="@dimen/_5pxh"
                        android:paddingTop="@dimen/_6pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_new2"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_1" tools:text="@string/lbl_new2"/>
                </FrameLayout>
            </FrameLayout>
            <LinearLayout android:id="@+id/linearRatingnopoin"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageStar"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_width="@dimen/_9pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                <ImageView android:id="@+id/imageStarOne"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                <ImageView android:id="@+id/imageStarTwo"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                <ImageView android:id="@+id/imageStarThree"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                <ImageView android:id="@+id/imageStarFour"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNumber"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.15"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{mainPageRowModel.txtNumber}"
                    style="@style/txtMetropolisregular10" tools:text="@string/lbl_0"/>
            </LinearLayout>
            <ImageButton
                android:background="@drawable/rectangle_bg_white_a700_radius_18"
                android:id="@+id/btnClock"
                android:layout_gravity="bottom|end"
                android:layout_height="@dimen/_36pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_36pxh"
                android:paddingHorizontal="@dimen/_11pxh"
                android:paddingVertical="@dimen/_11pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/custom_img_clock" tools:ignore="ContentDescription"/>
        </FrameLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtBrandname"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{mainPageRowModel.txtBrandname}"
            style="@style/txtMetropolisregular11_1" tools:text="@string/lbl_ovs"/>
        <TextView android:ellipsize="end" android:id="@+id/txtItem"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{mainPageRowModel.txtItem}"
            style="@style/txtMetropolissemibold16" tools:text="@string/lbl_blouse"/>
        <TextView android:ellipsize="end" android:id="@+id/txtPrice"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_2pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_6pxh"
            android:singleLine="true"
            android:text="@{mainPageRowModel.txtPrice}"
            style="@style/txtMetropolismedium14" tools:text="@string/lbl_30"/>
    </LinearLayout>
</layout>
