<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="myProfileVM" type="com.nguyenvansapplication.app.modules.myprofile.data.viewmodel.MyProfileVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearMyProfile"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_11pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <LinearLayout android:id="@+id/linearColumnsearch"
            android:layout_height="0dp" android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageView android:id="@+id/imageSearch"
                android:layout_gravity="end"
                android:layout_height="@dimen/_17pxh"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_width="@dimen/_17pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_search_gray_900"
                tools:ignore="ContentDescription" tools:src="@drawable/img_search_gray_900"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnheadline"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_33pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtHeadline"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{myProfileVM.myProfileModel.txtHeadline}"
                    style="@style/txtMetropolisbold34" tools:text="@string/lbl_my_profile"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowmatildabrown"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_17pxh"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageImage"
                        android:layout_height="@dimen/_64pxh"
                        android:layout_width="@dimen/_64pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        tools:ignore="ContentDescription" />
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnmatildabrown"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_25pxv"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.72"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtMatildaBrown"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            style="@style/txtMetropolissemibold18" tools:text="@string/lbl_matilda_brown"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtEmail"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            style="@style/txtMetropolismedium14_2" tools:text="@string/msg_matildabrown_ma"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowshippingaddres"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="50dp"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnshippingaddres"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.98"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtShippingaddres"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{myProfileVM.myProfileModel.txtShippingaddres}"
                            style="@style/txtMetropolissemibold16" tools:text="@string/msg_shipping_addres3"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowrightOne"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="@dimen/_7pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright_gray_500"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_gray_500"/>
                </LinearLayout>
                <View android:background="@color/gray_500_5e"
                    android:id="@+id/lineDividerOne"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_375pxh"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowpaymentmethods"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnpaymentmethods"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.98"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPaymentmethods"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{myProfileVM.myProfileModel.txtPaymentmethods}"
                            style="@style/txtMetropolissemibold16" tools:text="@string/lbl_payment_methods"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowrightTwo"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_7pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright_gray_500"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_gray_500"/>
                </LinearLayout>
                <View android:background="@color/gray_500_5e"
                    android:id="@+id/lineDividerTwo"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_375pxh"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowpromocodes"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnpromocodes"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.98"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPromocodes"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{myProfileVM.myProfileModel.txtPromocodes}"
                            style="@style/txtMetropolissemibold16" tools:text="@string/lbl_promocodes"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtYouhavespecia"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{myProfileVM.myProfileModel.txtYouhavespecia}"
                            style="@style/txtMetropolisregular11_1" tools:text="@string/msg_you_have_specia"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowrightThree"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="@dimen/_7pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright_gray_500"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_gray_500"/>
                </LinearLayout>
                <View android:background="@color/gray_500_5e"
                    android:id="@+id/lineDividerThree"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_375pxh"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowmyreviews"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnmyreviews"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.98"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtMyreviews"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="8dp"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{myProfileVM.myProfileModel.txtMyreviews}"
                            style="@style/txtMetropolissemibold16" tools:text="@string/lbl_my_reviews"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowrightFour"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_7pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright_gray_500"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_gray_500"/>
                </LinearLayout>
                <View android:background="@color/gray_500_5e"
                    android:id="@+id/lineDividerFour"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_375pxh"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearLine6"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnsettings"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.98"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSettings"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"

                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{myProfileVM.myProfileModel.txtSettings}"
                            style="@style/txtMetropolissemibold16" tools:text="@string/lbl_settings"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtNotifications"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{myProfileVM.myProfileModel.txtNotifications}"
                            style="@style/txtMetropolisregular11_1" tools:text="@string/msg_notifications"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowrightFive"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="@dimen/_7pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright_gray_500"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_gray_500"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
